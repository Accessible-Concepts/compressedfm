{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "no-return-await": "off",
    "react/jsx-props-no-spreading": "off",
    "jsx-a11y/media-has-caption": "off",
    "react/prop-types": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}